{
  "listen": "127.0.0.1:5520",
  "handlers": [
    {"type": "logsni"},
    {
      "type": "sni-router",
      "config": {
        "routes": {
          "test.local": "hybuild.net:5521"
        }
      }
    },
    {
      "type": "terminator",
      "config": {
        "listen": ":5522",
        "certs": {
          "default": {
            "cert": "server.crt",
            "key": "server.key"
          }
        },
        "debug": true,
        "debug_packet_limit": 100
      }
    },
    {"type": "forwarder"}
  ]
}
